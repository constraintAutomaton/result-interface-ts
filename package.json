{
  "name": "result-interface",
  "module": "index.ts",
  "type": "module",
  "author": {
    "name": "Bryan-Elliott Tam"
  },
  "homepage": "https://github.com/constraintAutomaton/result-interface-ts",
  "license": "MIT",
  "version": "1.0.7",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "type-safety",
    "typing",
    "error-handling",
    "typescript-utility",
    "safe-result",
